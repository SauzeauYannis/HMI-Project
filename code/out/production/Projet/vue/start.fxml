<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="800.0" minWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controleur.StartController">
   <Label layoutX="425.0" layoutY="30.0" text="Gypsy's Carnaval">
      <font>
         <Font name="Carlito Bold" size="50.0" />
      </font>
   </Label>
   <Button fx:id="playButton" layoutX="50.0" layoutY="700.0" mnemonicParsing="false" onMouseClicked="#playMouseClicked" onMouseEntered="#playMouseEntered" onMouseExited="#playMouseExited" prefWidth="500.0" text="Play">
      <font>
         <Font name="Carlito" size="30.0" />
      </font>
   </Button>
   <ImageView fitHeight="500.0" fitWidth="1000.0" layoutX="218.0" layoutY="138.0" pickOnBounds="true" preserveRatio="true">
      <Image url="@image/background.png" />
   </ImageView>
   <Button fx:id="quitButton" layoutX="629.0" layoutY="700.0" mnemonicParsing="false" onMouseClicked="#quitMouseClicked" onMouseEntered="#quitMouseEntered" onMouseExited="#quitMouseExited" prefWidth="500.0" text="Quit">
      <font>
         <Font name="Carlito" size="30.0" />
      </font>
   </Button>
</AnchorPane>
