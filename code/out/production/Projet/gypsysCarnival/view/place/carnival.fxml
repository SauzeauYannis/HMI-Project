<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import gypsysCarnival.view.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity"
            onMouseClicked="#gameSceneClicked" prefHeight="615.0" prefWidth="865.0"
            xmlns="http://javafx.com/javafx/8.0.171" fx:controller="gypsysCarnival.controller.place.CarnivalController">
    <ImageView fx:id="playerIcon" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@../design/image/benjapied.gif"/>
    </ImageView>
    <ClickableImage fx:id="keyShopIcon" fitHeight="150.0" fitWidth="150.0" layoutX="391.0" layoutY="427.0"
                    pickOnBounds="true" preserveRatio="true" tooltipText="Go to the key shop">
        <Image url="@../design/image/key_shop.gif"/>
    </ClickableImage>
    <ClickableImage fx:id="foodShopIcon" fitHeight="150.0" fitWidth="150.0" layoutX="50.0" layoutY="427.0"
                    pickOnBounds="true" preserveRatio="true" tooltipText="Go to the food shop">
        <Image url="@../design/image/food_shop.gif"/>
    </ClickableImage>
    <ClickableImage fx:id="sparklingCaravanIcon" fitHeight="150.0" fitWidth="150.0" layoutX="664.0" layoutY="427.0"
                    pickOnBounds="true" preserveRatio="true" tooltipText="Go to the sparkling caravan">
        <Image url="@../design/image/sparkling_caravan.gif"/>
    </ClickableImage>
    <ClickableImage fx:id="copperHubIcon" fitHeight="150.0" fitWidth="150.0" layoutX="227.0" layoutY="35.0"
                    pickOnBounds="true" preserveRatio="true" tooltipText="Go to the copper hub">
        <Image url="@../design/image/copper_hub.png"/>
    </ClickableImage>
    <ClickableImage fx:id="goldHubIcon" fitHeight="150.0" fitWidth="150.0" layoutX="460.0" layoutY="35.0"
                    pickOnBounds="true" preserveRatio="true" tooltipText="Go to the gold hub">
        <Image url="@../design/image/gold_hub.png"/>
    </ClickableImage>
    <ClickableImage fx:id="platinumHubIcon" fitHeight="150.0" fitWidth="150.0" layoutX="664.0" layoutY="35.0"
                    pickOnBounds="true" preserveRatio="true" tooltipText="Go to the platinum hub">
        <Image url="@../design/image/platinum_hub.png"/>
    </ClickableImage>
    <ImageView fx:id="padlockCaravanIcon" fitHeight="75.0" fitWidth="75.0" layoutX="695.0" layoutY="456.0"
               onMouseClicked="#padlockClicked" pickOnBounds="true" preserveRatio="true">
        <Image url="@../design/image/padlock.png"/>
    </ImageView>
</AnchorPane>
