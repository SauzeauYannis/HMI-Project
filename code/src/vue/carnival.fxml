<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controleur.CarnivalController"
            fx:id="carnivalScene" minHeight="640.0" minWidth="865.0" onMouseClicked="#gameSceneClicked">
    <Button layoutX="396.0" layoutY="440.0" mnemonicParsing="false"
            onMouseClicked="#testBuyKeyClicked" text="Acheter clef"/>
    <ImageView fx:id="benjapiedIcon" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@image/benjapied.gif"/>
    </ImageView>
</AnchorPane>